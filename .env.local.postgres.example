# PostgreSQL APL Configuration Example
# Copy this to .env.local and update with your values

APP_IFRAME_BASE_URL=http://localhost:3000
APP_API_BASE_URL=http://localhost:3000

# Your Saleor instance URL
NEXT_PUBLIC_SALEOR_API_URL=https://your-saleor-instance.saleor.cloud/graphql/

# Channel slug
NEXT_PUBLIC_DEFAULT_CHANNEL=default-channel

# Use PostgreSQL APL
APL=postgres

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=saleor_app
DB_USER=postgres
DB_PASSWORD=your_password_here
